# Healthcare Agent Environment Configuration
# Copy this file to .env and update with your actual values

# Application Settings
APP_ENV=development
APP_DEBUG=true
APP_PORT=3000

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=healthcare_agent
DB_USER=your_db_user
DB_PASSWORD=your_db_password

# Healthcare API Settings
HEALTHCARE_API_URL=https://api.healthcare-provider.com
HEALTHCARE_API_KEY=your_api_key_here
HEALTHCARE_API_SECRET=your_api_secret_here

# Audit & Logging
LOG_LEVEL=info
AUDIT_ENABLED=true
AUDIT_LOG_PATH=./data/logs/audit.log

# Security
JWT_SECRET=your_jwt_secret_key_here
ENCRYPTION_KEY=your_encryption_key_here

# Playwright Configuration
PLAYWRIGHT_HEADLESS=true
PLAYWRIGHT_TIMEOUT=30000

# Email Configuration (optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_email_password

# Redis (optional, for caching)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
